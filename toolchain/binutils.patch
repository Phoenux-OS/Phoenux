diff -urN binutils-2.32/ld/emulparams/elf_i386_phoenux.sh binutils-2.32-patched/ld/emulparams/elf_i386_phoenux.sh
--- binutils-2.32/ld/emulparams/elf_i386_phoenux.sh	1970-01-01 01:00:00.000000000 +0100
+++ binutils-2.32-patched/ld/emulparams/elf_i386_phoenux.sh	2019-09-28 00:18:14.071642011 +0200
@@ -0,0 +1,3 @@
