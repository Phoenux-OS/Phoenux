+SCRIPT_NAME=i386phoenux
+TEXT_START_ADDR=0x10000
diff -urN binutils-2.32/ld/scripttempl/i386phoenux.sc binutils-2.32-patched/ld/scripttempl/i386phoenux.sc
--- binutils-2.32/ld/scripttempl/i386phoenux.sc	1970-01-01 01:00:00.000000000 +0100
+++ binutils-2.32-patched/ld/scripttempl/i386phoenux.sc	2019-09-28 02:00:35.378481353 +0200
@@ -0,0 +1,27 @@
+cat <<EOF
+
+OUTPUT_FORMAT(binary)
+ENTRY(_start)
+
+SECTIONS {
+    . = 0x10000;
+
+    .text ALIGN(4K) : {
+        KEEP(*(.phoenux_entry))
+        *(.text*)
+    }
+
+    .got ALIGN(4K) : {
+        _GLOBAL_OFFSET_TABLE_ = .;
+        KEEP(*(.got*))
+    }
+
+    .data ALIGN(4K) : {
+        *(.data*)
+        *(.rodata*)
+        *(.bss*)
+        *(COMMON)
+    }
+}
+
+EOF
